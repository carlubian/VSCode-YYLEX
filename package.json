{
    "name": "yylex",
    "displayName": "VSCode-YYLEX",
    "description": "Provides support for yylex files within Visual Studio Code",
    "version": "1.1.3",
    "publisher": "carlubian",
    "icon": "images/icon-YYLEX.png",
    "homepage": "https://github.com/carlubian/VSCode-YYLEX",
    "galleryBanner": {
        "color": "#007ACC",
        "theme": "dark"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/carlubian/VSCode-YYLEX.git"
    },
    "engines": {
        "vscode": "^1.5.0"
    },
    "categories": [
        "Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "yylex",
            "aliases": ["YYLEX", "yylex"],
            "extensions": [".l"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "yylex",
            "scopeName": "text.java.yylex",
            "path": "./syntaxes/yylex.tmLanguage.json"
        }]
    }
}
